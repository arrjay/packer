{
  "variables": {
    "iso_url": "http://mko.wcs.bbxn.us/ubuntu-releases/16.10/ubuntu-16.10-server-amd64.iso",
    "iso_checksum": "72b0d421da77f1e0c549b4efe6fc6c184e9909d6792f0d1e59b56d63e9705659",
    "iso_checksum_type": "sha256"
  },
  "builders": [
    {
      "type": "qemu",
      "iso_url": "{{ user `iso_url` }}",
      "iso_checksum": "{{ user `iso_checksum` }}",
      "iso_checksum_type": "{{ user `iso_checksum_type` }}",
      "ssh_username": "packer",
      "ssh_password": "packer",
      "ssh_timeout": "30m",
      "http_directory": "ubu-files",
      "boot_command": "<esc><wait><esc><wait><enter><wait>/install/vmlinuz initrd=/install/initrd.gz auto-install/enable=true debconf/priority=critical preseed/url=http://{{.HTTPIP}}:{{.HTTPPort}}/preseed.cfg<enter>"
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "inline": "true"
    }
  ]
}
